{
  "name": "magical-tavern",
  "version": "2.0.0",
  "author": "Dalton Wais",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "node server.js",
    "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "reinstall": "rm -rf node_modules && rm -rf package-lock.json && npm install",
    "debug": "node inspect server.js",
    "kill": "lsof -P | grep ':5001' | awk '{print $2}' | xargs kill -9",
    "migrate": "node-pg-migrate",
    "migrate.dev.up": "DATABASE_URL=postgres://localhost:5432/magical-tavern-db node-pg-migrate up",
    "migrate.dev.down": "DATABASE_URL=postgres://localhost:5432/magical-tavern-db node-pg-migrate down"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "node-pg-migrate": "^3.18.1",
    "pg": "^7.10.0",
    "pg-async": "^3.1.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.0.5"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
